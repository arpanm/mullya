<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity Requirement.
    -->
    <changeSet id="20211126181623-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="address_id"
                                 baseTableName="requirement"
                                 constraintName="fk_requirement__address_id"
                                 referencedColumnNames="id"
                                 referencedTableName="address"/>

        <addForeignKeyConstraint baseColumnNames="actor_id"
                                 baseTableName="requirement"
                                 constraintName="fk_requirement__actor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="actor"/>
    </changeSet>
</databaseChangeLog>
